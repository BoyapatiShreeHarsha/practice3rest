    "axios-mock-adapter": "^1.22.0",
    "country-iso-3-to-2": "^1.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
